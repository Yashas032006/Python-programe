        "  return first.count(element)\n",
        "  numbers = [1,2,3,4,2,2,5,6,2]\n",
        "  element_to_count = 2\n",
        "  count = count_occurrences(numbers, element_to_count)\n",
        "  print(f\"the element {element_to_count} appears {count} times in the list.\")"
