      "source": [
        "def merge_lists_remove_duplicates(list1, list2):\n",
        "\n",
        "    merged_list = list(dict.fromkeys(list1 + list2))\n",
        "    return merged_list\n",
        "\n",
        "\n",
        "list1 = [1, 2, 3, 4, 5]\n",
        "list2 = [3, 4, 5, 6, 7]\n",
        "\n",
        "result = merge_lists_remove_duplicates(list1, list2)\n",
        "print(\"Merged list without duplicates:\", result)\n"
